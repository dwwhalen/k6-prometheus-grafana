{
  "dashboard": {
    "id": null,
    "title": "K6 Load Testing Dashboard",
    "tags": ["k6", "load-testing"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Virtual Users",
        "type": "stat",
        "targets": [
          {
            "expr": "k6_vus",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "k6_http_req_duration{quantile=\"0.95\"}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 0}
      }
    ],
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "refresh": "5s"
  }
}